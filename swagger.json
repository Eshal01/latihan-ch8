{
  "openapi": "3.0.3",
  "info": {
    "title": "BINAR CHALLENGE CHAPTER 8",
    "description": "Challenge dokumentasi API Chapter 8",
    "contact": {
      "email": "mahardika1987@gmail.com"
    },
    "version": "1.0.11"
  },
  "servers": [
    {
      "url": "http://localhost:4000/api/v1"
    }
  ],
  "paths": {
    "/players": {
      "get": {
        "summary": "Get All Player Game",
        "description": "Get All Data Player",
        "produces": "application/json",
        "responses": {
          "200": {
            "description": "Get Player Success"
          }
        }
      },
      "post": {
        "summary": "Create Player",
        "description": "Create Player Game",
        "consumes": "multipart/form-data",
        "produces": "application/json",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "username": {
                    "type": "string"
                  },
                  "email": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  },
                  "experience": {
                    "type": "integer"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Create Player Success"
          }
        }
      }
    }
  }
    
}